        +b.slider-pag

    +pagepiling-slide
      +b.slide2.slide
        +e.box
          +b.advantage-wrap
            +b.advantage
              +b.advantage-inner
                +icon('plus').__plus
                +e.num 7
              +e.text 7 лет успешной<br> работы
            +b.advantage
              +b.advantage-inner
                +icon('plus').__plus
                +e.num 200
              +e.text Выполнено более <br>200 проектов
