              img(src='/assets/images/slide1c.png').slide__center
            +slide
              .slide__bg
                img(src='/assets/images/slide2l.jpg').slide__left
                img(src='/assets/images/slide2r.jpg').slide__right
              img(src='/assets/images/slide2c.png').slide__center
              .slide__plate
                .slide__title Разработка интернет-магазина<br>«Ангел и демон»
