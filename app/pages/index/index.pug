    +pagepiling-slide
      +b.slide5.slide
        +e.box
          +e.container
            +b.city-wrap
              +b.city_piter.js-city
                +e.H3.title
                  +icon('spb').__nohover
                  +e.name_piter Петербург
                +e.address 196247,<br>Ленинский проспект, дом 151<br>офис 315
                +link(href='#')._white._xl.city__map Как проехать
                +e.tel  +7 (812) 645-65-96
              +b.city_moscow.js-city
