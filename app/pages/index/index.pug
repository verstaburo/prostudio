              +e.scroll
                +e.IMG(src='./assets/images/mouse_grey.svg').mouse
                +e.text листайте вниз
              +e.portfolio
                a(href="portfolio.html").index-link портфолио
        +e.slider.slider_portfolio
          +slider(false, false)
            +slide-project('./assets/images/slide1l.jpg', './assets/images/slide1r.jpg', './assets/images/slide1c.png', 'Разработка интернет-магазина<br>«Swimchamp»', 'WEB')
            +slide-project('./assets/images/slide2l.jpg', './assets/images/slide2r.jpg', './assets/images/slide2c.png', 'Разработка интернет-магазина<br>«Ангел и демон»', 'WEB')
